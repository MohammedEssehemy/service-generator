{
  "ecmaVersion": "7",
  "libs": [
    "chai",
    "browser",
    "jquery",
    "underscore"
  ],
  "loadEagerly": [],
  "dontLoad": [
    "node_modules/**",
    ".meteor/**"
  ],
  "plugins": {
    "doc_comment": true,
    "complete_strings": {
      "maxLength": 15
    },
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "node_resolve": {},
    "es_modules": {},
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "commonjs": {}
  }
}